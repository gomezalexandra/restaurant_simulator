{% extends 'base.html.twig' %}
{% block title %} workflow {% endblock %}

{% block container %}

    {% for pr in prs %}
        <h1>{{ pr.title }}</h1>

        {% for transition in workflow_transitions(pr) %}
            <a href="#">{{ transition.name }}</a>
        {% else %}
            No actions available.
        {% endfor %}
    {% endfor %}

{% endblock %}